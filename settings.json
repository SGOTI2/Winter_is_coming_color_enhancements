{
    "editor.semanticTokenColorCustomizations": {
        "[Winter*]": {
            "enabled": true,
            "rules": {
                "method": {
                    "fontStyle": "italic bold"
                },
                "function": {
                    "fontStyle": "italic bold"
                },
                "variable": {
                    "foreground": "#dd8888",
                    "fontStyle": "bold"
                },
                "magicFunction": {
                    "fontStyle": "italic"
                },
                "method.declaration": {
                    "fontStyle": "bold"
                },
                "function.declaration": {
                    "fontStyle": "bold"
                },
                "module": {
                    "foreground": "#9a88dd",
                    "fontStyle": "italic",
                },
                "class": {
                    "foreground": "#c2a478",
                    "fontStyle": "bold"
                },
                "enumMember": {
                    "foreground": "#d29ffc",
                    "fontStyle": "italic bold"
                },
                "selfParameter": {
                    "foreground": "#AAA",
                    "fontStyle": "italic"
                }
            }
        }
    },
    "editor.tokenColorCustomizations": {
        "[Winter*]": {
            "textMateRules": [
                {
                    "scope": "storage.type",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "constant.other",
                    "settings": {
                        "fontStyle": "italic bold"
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Winter is Coming (Dark Blue*]": {
            "statusBar.background":"#081120",
            "statusBarItem.hoverBackground": "#0d1b33",
            "widget.shadow": "#171717",
            "menu.background": "#121d33",
            "editorWidget.background": "#121d33",
            "editorWidget.border": "#202c46",
            "editorSuggestWidget.background": "#121d33",
            "editorHoverWidget.background": "#121d33",
            "editorHoverWidget.border": "#202c46",
            "panel.border": "#219fd544",
            "input.background": "#121d33",
            "input.border": "#202c46",
            "inputOption.activeBorder": "#202c46",
            
        }
    },
}
